{
	"manifest_version": 2,
	"name": "Impatient IMSLP",
	"description": "Bypass IMSLP's 15 second download waiting time.",
	"version": "1.0",
	
	"icons": {
		"128": "icon.png",
	    "48": "icon.png",
	    "16": "icon.png"
	},	

	"content_scripts": [
		{
	    	"matches": ["http://imslp.org/wiki/Special:ImagefromIndex/*"],
	    	"js": ["popup.js"],
	    	"css": ["imslp.css"]
	    }	   
	],

	"permissions": [
		"activeTab"
	]
}